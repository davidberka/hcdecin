  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.0",
    "postcss-cli": "^9.0.2",
    "tailwindcss": "^2.2.19",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "concurrently": "^6.3.0"
  }
}